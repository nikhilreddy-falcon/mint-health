{
  "name": "acumatica-analytics-platform",
  "version": "1.0.0",
  "description": "Analytics platform for Acumatica ERP with dummy data generation",
  "main": "src/server/index.js",
  "scripts": {
    "generate-data": "node src/data-generation/generateData.js",
    "start-api": "node src/api/server.js",
    "start-analytics": "node src/analytics/server.js",
    "start": "concurrently \"npm run start-api\" \"npm run start-analytics\" \"npm run start-client\"",
    "start-client": "cd client && npm start",
    "install-all": "npm install && cd client && npm install",
    "build": "cd client && npm run build"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "faker": "^5.5.3",
    "uuid": "^9.0.0",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "helmet": "^7.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  }
}
